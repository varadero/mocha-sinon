{
  "name": "testing",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "tsc && mocha tsc-out/test/**/*.test.js",
    "test:report": "tsc && mocha tsc-out/test/**/*.test.js --reporter mochawesome --reporter-options reportDir=test-results",
    "test:coverage": "tsc && nyc --extension .ts --reporter=lcov --reporter=text --exclude-after-remap=false --include=tsc-out/src/**/*.js --all=true --cache=true --report-dir=target/coverage mocha tsc-out/test/**/*.test.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.0.0",
    "@types/sinon": "^5.0.0",
    "chai": "4.1.2",
    "es6-promise": "^4.2.4",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.0",
    "nock": "^9.3.2",
    "nyc": "^12.0.0",
    "sinon": "^5.0.0",
    "source-map-support": "^0.5.6",
    "typescript": "^2.9.1"
  }
}
